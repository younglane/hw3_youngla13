CREATE TABLE Book (
	bookID int, --to create bookID
	ISBN varchar,
	title varchar(40),
	author varchar(255),
	publish-year DATE, 
	category varchar(255)
	PRIMARY KEY (bookID));
	
CREATE TABLE Member (
	memberID int,
	lastname varchar(255),
	firstname varchar(255),
	address varchar(255),
	phone_number varchar(255),
	limit = 10
	PRIMARY KEY (memberID));

CREATE TABLE CurrentLoan (
	memberID int,
	bookID int,
	loan_date DATE,
	due_date DATE
	PRIMARY KEY (memberID, bookID)
	FOREIGN KEY (memberID, bookID));
	
CREATE TABLE History (
	memberID int,
	bookID int,
	loan_date DATE,
	return_date DATE);
	
